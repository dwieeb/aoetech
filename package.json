{
  "name": "aoetech",
  "version": "0.0.0",
  "description": "Canvas version of Age of Empires technology tree.",
  "main": "index.html",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass": "node-sass ./scss/main.scss ./css/bundle.css",
    "sass:w": "node-sass -w ./scss/main.scss ./css/bundle.css",
    "build": "concurrent \"npm run tsc\" \"npm run sass\" ",
    "build:w": "concurrent \"npm run tsc:w\" \"npm run sass:w\" "
  },
  "repository": {
    "type": "git",
